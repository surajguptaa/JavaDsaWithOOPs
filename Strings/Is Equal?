import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.nextLine();
        String b = sc.nextLine();
        boolean ans = false;
        if(a.length()==b.length()){
                if(a.contains(b)){
                    ans = true;
               } 
        }
        System.out.println(ans);
    }
}
